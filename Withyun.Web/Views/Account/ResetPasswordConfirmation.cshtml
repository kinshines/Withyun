
@{
    ViewBag.Title = "密码重置成功";
    Layout = "~/Views/Shared/_LayoutEmpty.cshtml";
    ViewBag.BodyClass = "login1";
}

<!-- Login Screen -->
<div class="login-wrapper">
    <div class="login-container">
        <a href="@Url.Action("Index","Home")">
            <img src="~/images/logo-login@2x.png" />
        </a>
        <br />
        <h4>新密码已成功设置</h4>
        <p>现在，您可以登录网站，分享你的资料，获得收益</p>
        <br />
        <div class="social-login clearfix">
            @Html.ActionLink("现在进入", "Index", "Home", null, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>
<!-- End Login Screen -->
