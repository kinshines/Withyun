
@{
    ViewBag.Title = "您发布的分享正在审核中……";
}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="widget-container fluid-height clearfix">
            <div class="heading">
                <i class="fa fa-check"></i>@ViewBag.Title
            </div>
            <hr />
            <div class="widget-content padded">
                <p>
                    <a asp-action="Verify" class="btn btn-primary">查看待审核的分享</a>
                    <a asp-action="Report" class="btn btn-success">查看内容违规的分享</a>
                    <a asp-action="Manage" class="btn btn-info">管理我的分享</a>
                </p>
            </div>
        </div>
    </div>
</div>

